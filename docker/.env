# Password for the 'elastic' user (at least 6 characters)
ELASTIC_PASSWORD=elastic
 
 
# Version of Elastic stack products
STACK_VERSION=8.12.1
 
# Set the cluster name
CLUSTER_NAME=es-docker-cluster
 
# Set to 'basic' or 'trial' to automatically start the 30-day trial
LICENSE=basic
#LICENSE=trial
 
# Port to expose Elasticsearch HTTP API to the host
ES_PORT=9200
#ES_PORT=127.0.0.1:9200
 
# Port to expose Kibana to the host
KIBANA_PORT=5601

# Port to expose Logstash to the filebeat
LOGSTASH_FB_PORT=5044
 
# Increase or decrease es memory limit based on the available host memory (in bytes)
ES_MEM_LIMIT=1073741824

# Increase or decrease kibana memory limit based on the available host memory (in bytes)
KB_MEM_LIMIT=1073741824
 
# Project namespace (defaults to the current folder name if not set)
#COMPOSE_PROJECT_NAME=myproject